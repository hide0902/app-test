.main
  = search_form_for @search, url: root_path do |f|
    .main__1
      = f.label :name_cont, "keyword"
      = f.search_field :name_cont
    .main__2
      = f.search_field :rent_gteq, placeholder: "¥ Min"
      %p ~
      = f.search_field :rent_lteq, placeholder: "¥ Min"
    .main__3
      = f.submit "検索"




- @lists.each do |list|
  - list.stations.each do |station|
    - list.images.each do |image|
      = list.name
      = list.street
      = list.rent
      = station.station
      = image_tag image.image_sub, height: '150px', width: '220px'